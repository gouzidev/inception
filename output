WORDPRESS_DB_PASSWORD=salah

WORDPRESS_TABLE_PREFIX=wp_
=== ./srcs/requirements/wordpress/tools/start.sh ===


mkdir -p /var/data/www

# installing wordpress to its right path
wget https://wordpress.org/latest.tar.gz -O /tmp/wordpress.tar.gz \
  && tar -xzf /tmp/wordpress.tar.gz -C /var/data/www/ \
  && rm /tmp/wordpress.tar.gz

cp /tmp/wp-config.php /var/data/www/wordpress/wp-config.php

rm /tmp/wp-config.php

chown -R nobody:nobody /var/data/www/wordpress


echo "starting php-fpm..."

exec php-fpm83 -F
=== ./srcs/requirements/wordpress/Dockerfile ===


FROM alpine:3.21.4

# tools first (alpine is minimal)
RUN apk add --no-cache wget tar  curl zsh



#installing php and php-fpm
RUN apk add --no-cache php php-fpm php-mysqli php-pdo php-pdo_mysql php-json php-curl php-gd php-zip php-xml php-mbstring php-session php-fileinfo php-tokenizer 

RUN apk add --no-cache mariadb-client


# Configure php-fpm to listen on port 9000
COPY conf/php-fpm.conf /etc/php83/php-fpm.d/www.conf


COPY conf/wp-config.php /tmp/wp-config.php


COPY tools/start.sh ./script.sh

# this will RUN it not as shell (exec mode not shell mode) so it will get pid 1
ENTRYPOINT ./script.sh
=== ./srcs/requirements/nginx/conf/conf.conf ===



worker_processes auto;

error_log /var/log/nginx/error.log debug;

include /etc/nginx/conf.d/*.conf;

events
{
        # The maximum number of simultaneous connections that can be opened by
        # a worker process.
        worker_connections 1024;
}


http
{
    include /etc/nginx/mime.types;
    sendfile on;
    server
    {
        listen 443 ssl;
        listen [::]:443 ssl;
        server_name sgouzi.42.fr;
        root /var/data/www/wordpress;
        index index.php index.html index.htm;

        ssl_certificate /etc/nginx/ssl/nginx.crt;
        ssl_certificate_key /etc/nginx/ssl/nginx.key;

        ssl_protocols TLSv1.2 TLSv1.3;

        location /
        {
            try_files $uri $uri/ /index.php?$args;
        }


        location ~ \.php$ {
            try_files $uri =404;
            fastcgi_split_path_info ^(.+\.php)(/.+)$;
            fastcgi_pass wordpress:9000;
            fastcgi_index index.php;
            fastcgi_param SCRIPT_FILENAME $document_root$fastcgi_script_name;
            include fastcgi_params;
        }

    }
}
=== ./srcs/requirements/nginx/conf/hosts ===


127.0.0.1       localhost
127.0.0.1       sgouzi.42.fr
=== ./srcs/requirements/nginx/Dockerfile ===


FROM alpine:3.21.4


# tools first (alpine is minimal)
RUN apk add --no-cache openssl zsh curl bash


#install nginx
RUN apk add --no-cache nginx



# setup certificate
RUN mkdir -p /etc/nginx/ssl

RUN openssl req -x509 -nodes -days 365 -newkey rsa:2048 \
    -keyout /etc/nginx/ssl/nginx.key \
    -out /etc/nginx/ssl/nginx.crt \
    -subj "/C=US/ST=State/L=City/O=Organization/CN=sgouzi.42.fr" \
    -addext "subjectAltName=DNS:sgouzi.42.fr,DNS:localhost,IP:127.0.0.1"

RUN chmod 600 /etc/nginx/ssl/nginx.key

RUN chmod 644 /etc/nginx/ssl/nginx.crt



COPY conf/conf.conf /etc/nginx/nginx.conf
# COPY the config file FROM host to the container for nginx to use it

RUN echo "starting nginx..."

# RUN it as entry point (exec mode)
ENTRYPOINT ["nginx", "-g", "daemon off;"]
=== ./srcs/requirements/mariadb/conf/db.cnf ===


# tools/my.cnf
[mysqld]
user = mysql
datadir = /var/lib/mysql
socket = /run/mysqld/mysqld.sock
bind-address = 0.0.0.0
port = 3306
character-set-server = utf8mb4
collation-server = utf8mb4_unicode_ci
skip-name-resolve

[client]
socket = /run/mysqld/mysqld.sock
=== ./srcs/requirements/mariadb/conf/.env ===



MARIADB_DATABASE=inception

MARIADB_ADMIN_USERNAME=sgouzi
MARIADB_ADMIN_PASSWORD=sgouzi

MARIADB_USER_USERNAME=salah
MARIADB_USER_PASSWORD=salah





=== ./srcs/requirements/mariadb/tools/init.sh ===


#!/bin/sh
# tools/init.sh

set -e  # Exit if any command fails

# Get environment variables (with defaults)
MARIADB_ADMIN_PASSWORD="${MARIADB_ADMIN_PASSWORD}"
MARIADB_DATABASE="${MARIADB_DATABASE}"

MARIADB_USER_USERNAME="${MARIADB_USER_USERNAME}"
MARIADB_USER_PASSWORD="${MARIADB_USER_PASSWORD}"

DATADIR="/var/lib/mysql"
SOCKET="/run/mysqld/mysqld.sock"

echo "ğŸ”§ MariaDB: Starting initialization..."


# Ensure proper ownership
chown -R mysql:mysql /var/lib/mysql /run/mysqld

# Check if this is the first run
if [ ! -d "$DATADIR/mysql" ]; then
    echo "ğŸ—ï¸ MariaDB: First RUN detected, initializing database..."
    
    # Initialize the database system
    # This creates the mysql, performance_schema, etc. databases
    mariadb-install-db \
        --user=mysql \
        --datadir="$DATADIR" \
        --skip-test-db
    
    echo "ğŸš€ MariaDB: Starting temporary server for setup..."
    
    # Start MariaDB temporarily (socket only, no network)
    mysqld --user=mysql --datadir="$DATADIR" --socket="$SOCKET" --skip-networking &
    TEMP_PID=$!
    
    # Wait for MariaDB to be ready
    echo "â³ MariaDB: Waiting for server to be ready..."
    for i in $(seq 1 30); do
        if mysqladmin --socket="$SOCKET" ping >/dev/null 2>&1; then
            echo "âœ… MariaDB: Server is ready!"
            break
        fi
        sleep 1
    done
    
    # Check if server actually started
    if ! mysqladmin --socket="$SOCKET" ping >/dev/null 2>&1; then
        echo "âŒ MariaDB: Failed to start temporary server"
        exit 1
    fi
    
    echo "ğŸ“ MariaDB: Running initialization SQL..."
    
    #setup db
    mysql --socket="$SOCKET" <<-EOF
-- Set root password
ALTER USER 'root'@'localhost' IDENTIFIED BY '$MARIADB_ADMIN_PASSWORD';

-- Create root user for external connections
CREATE USER '$MARIADB_ADMIN_USERNAME'@'%' IDENTIFIED BY '$MARIADB_ADMIN_PASSWORD';
GRANT ALL PRIVILEGES ON *.* TO '$MARIADB_ADMIN_USERNAME'@'%' WITH GRANT OPTION;

-- Create application database
CREATE DATABASE IF NOT EXISTS \`$MARIADB_DATABASE\` 
    CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci;

-- Create application user
CREATE USER '$MARIADB_USER_USERNAME'@'%' IDENTIFIED BY '$MARIADB_USER_PASSWORD';
GRANT ALL PRIVILEGES ON \`$MARIADB_DATABASE\`.* TO '$MARIADB_USER_USERNAME'@'%';

-- Apply changes
FLUSH PRIVILEGES;
EOF


    echo "ğŸ›‘ MariaDB: Shutting down temporary server..."
    mysqladmin --socket="$SOCKET" --password="$MARIADB_ADMIN_PASSWORD" shutdown
    wait $TEMP_PID
    
    echo "âœ… MariaDB: Initialization complete!"
else
    echo "ğŸ“‚ MariaDB: Database already exists, skipping initialization"
fi

echo  "starting mariadb..."

# start maria process, replaces curr shell
exec mysqld --user=mysql --datadir="$DATADIR" --socket="$SOCKET" --bind-address="0.0.0.0"
=== ./srcs/requirements/mariadb/Dockerfile ===


FROM alpine:3.21.4


# tools first (alpine is minimal)
RUN apk add --no-cache wget tar  curl zsh 

# download mariadb
RUN apk add mariadb mariadb-client

# prepare needed folders for maria
RUN mkdir -p /var/lib/mysql /run/mysqld

RUN chown -R mysql:mysql /var/lib/mysql /run/mysqld



# COPY config file
COPY conf/db.cnf /etc/my.cnf


# COPY init script file
COPY tools/init.sh /usr/local/bin

ENTRYPOINT ["/usr/local/bin/init.sh"]
=== ./srcs/docker-compose.yml ===


services:

    wordpress:
        build: ./requirements/wordpress
        image: wordpress
        env_file:
            - ".env"
            - "./requirements/wordpress/conf/.env"
        volumes:
            - wordpress:/var/data/www
        networks:
            - docker-network
        dns:
            - 8.8.8.8
            - 1.1.1.1
        expose:
            - 9000
        restart: always
        depends_on:
            - mariadb

    nginx:
        build: ./requirements/nginx
        image: nginx
        volumes:
            - wordpress:/var/data/www
        networks:
            - docker-network
        ports:
            - 443:443
        restart: always
        env_file:
            - ".env"
            - "./requirements/mariadb/conf/.env"


    mariadb:
        build: ./requirements/mariadb
        image: mariadb
        volumes:
            - mariadb:/var/lib/mysql
        expose:
            - 3306
        env_file:
            - ".env"
            - "./requirements/mariadb/conf/.env"
        networks:
            - docker-network
        restart: always




networks:
    docker-network: {}

volumes:
    wordpress:
        driver: local
        driver_opts:
            type: none
            device: /home/sgouzi/data/wordpress
            o: bind
    mariadb:
        driver: local
        driver_opts:
            type: none
            device: /home/sgouzi/data/mariadb
            o: bind
