=== ./srcs/requirements/tools/add-host.sh ===


#!/bin/sh

HOST_ENTRY="127.0.0.1 $(DOMAIN)"

# Check if line already exists in /etc/hosts
if ! grep -qF "$HOST_ENTRY" /etc/hosts; then
    echo "$HOST_ENTRY" | sudo tee -a /etc/hosts > /dev/null
fi
=== ./srcs/requirements/wordpress/conf/wp-config.php ===


<?php
// tools/wp-config.php
/** The name of the database for WordPress */
define( 'DB_USER', getenv('WORDPRESS_DB_USER'));
define( 'DB_NAME', getenv('WORDPRESS_DB_NAME'));
define( 'DB_PASSWORD', getenv('WORDPRESS_DB_PASSWORD'));
define( 'DB_HOST', getenv('WORDPRESS_DB_HOST'));
define( 'DB_CHARSET', 'utf8mb4' );
define( 'DB_COLLATE', '' );

define('WP_ALLOW_REPAIR', true);

/** WordPress unique keys and salts */
define('AUTH_KEY',         'your-unique-auth-key-here');
define('SECURE_AUTH_KEY',  'your-unique-secure-auth-key-here');
define('LOGGED_IN_KEY',    'your-unique-logged-in-key-here');
define('NONCE_KEY',        'your-unique-nonce-key-here');
define('AUTH_SALT',        'your-unique-auth-salt-here');
define('SECURE_AUTH_SALT', 'your-unique-secure-auth-salt-here');
define('LOGGED_IN_SALT',   'your-unique-logged-in-salt-here');
define('NONCE_SALT',       'your-unique-nonce-salt-here');

/** WordPress database table prefix */
$table_prefix = 'wp_';

/** WordPress debugging mode */
define( 'WP_DEBUG', false );

/* That's all, stop editing! */
if ( ! defined( 'ABSPATH' ) ) {
    define( 'ABSPATH', __DIR__ . '/' );
}

require_once ABSPATH . 'wp-settings.php';
=== ./srcs/requirements/wordpress/conf/php-fpm.conf ===


[www]
user = nobody
group = nobody
listen = 0.0.0.0:9000
pm = dynamic
pm.max_children = 5
pm.start_servers = 2
pm.min_spare_servers = 1
pm.max_spare_servers = 3
pm.max_requests = 500

; Security
php_admin_value[disable_functions] = exec,passthru,shell_exec,system
php_admin_flag[allow_url_fopen] = off


; for some reason, php fpm doesnt pass my env variables to php scripts, so here im teaching him
clear_env = no
env[WORDPRESS_DB_HOST] = $WORDPRESS_DB_HOST
env[WORDPRESS_DB_NAME] = $WORDPRESS_DB_NAME
env[WORDPRESS_DB_USER] = $WORDPRESS_DB_USER
env[WORDPRESS_DB_PASSWORD] = $WORDPRESS_DB_PASSWORD
env[WORDPRESS_TABLE_PREFIX] = $WORDPRESS_TABLE_PREFIX

; Performance
php_value[memory_limit] = 128M
php_value[max_execution_time] = 30
